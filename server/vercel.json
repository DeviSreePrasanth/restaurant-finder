{
  "rewrites": [
    {
      "source": "/restaurants",
      "destination": "https://dsp-1.onrender.com/restaurants"
    },
    {
      "source": "/restaurants/:id",
      "destination": "https://dsp-1.onrender.com/restaurants/:id"
    },
    {
      "source": "/restaurants/locationR",
      "destination": "https://dsp-1.onrender.com/locationR?lat=${lat}&lng=${lng}&radius=${radius}"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "X-Requested-With, Content-Type, Accept"
        }
      ]
    }
  ]
}